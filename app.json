{
  "expo": {
    "name": "Wander",
    "slug": "Wander",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleMapsApiKey": "AIzaSyDTcF6iZAQh6ggH_Oa3Docc_ZEsFQwMD2c",
      "bundleIdentifier": "com.anonymous.Wander"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleMapsApiKey": "AIzaSyDTcF6iZAQh6ggH_Oa3Docc_ZEsFQwMD2c"
      },
      "package": "com.anonymous.Wander",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDTcF6iZAQh6ggH_Oa3Docc_ZEsFQwMD2c"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa4e211c-708b-484d-8de9-3f8df8cf8b5d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fa4e211c-708b-484d-8de9-3f8df8cf8b5d"
    }
  }
}
